    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "react-sizeme": "^3.0.2",
    "styled-components": "^5.3.3",
    "typeface-work-sans": "^1.1.13"
  },
  "devDependencies": {
    "react-snap": "^1.23.0",
    "serve": "^12.0.1",
    "source-map-explorer": "^2.4.2",
    "webpack": "^5.67.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
